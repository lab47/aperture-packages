, "sqlite" as i107, "subversion" as i108, "swig" as i109, "uchardet" as i110, "unzip" as i111, "utf8proc" as i112, "util-macros" as i113, "vala" as i114, "webp" as i115, "xcb-proto" as i116, "xorgproto" as i117, "xtrans" as i118, "xz" as i119, "zip" as i120, "zlib" as i121, "zstd" as i122

pkg(
