formula = r"""
class NicotinePlus < Formula
  include Language::Python::Virtualenv

  desc "Graphical client for the Soulseek file sharing network"
  homepage "https://nicotine-plus.github.io/nicotine-plus/"
  url "https://files.pythonhosted.org/packages/7b/15/a2bd4586013d77252ae354c8e6ec08464801c717255a0d3a7b3dff79f786/nicotine-plus-3.0.6.tar.gz"
  sha256 "06c6456c71943b35b3b23d3d958ab8c34e6471da0e46a615ba53dfb4db2a408c"
  license "GPL-3.0-or-later"
  head "https://github.com/Nicotine-Plus/nicotine-plus.git"

  bottle do
    sha256 cellar: :any_skip_relocation, arm64_big_sur: "dbcd856e88efa6227030bf3068d523280a036099f6439f0967904f10f104c126"
    sha256 cellar: :any_skip_relocation, big_sur:       "9a339a40b2a56caf796f0234a37d3db76d0954c3d2e03ae2fdd13b38198e7ac4"
    sha256 cellar: :any_skip_relocation, catalina:      "9a339a40b2a56caf796f0234a37d3db76d0954c3d2e03ae2fdd13b38198e7ac4"
    sha256 cellar: :any_skip_relocation, mojave:        "9a339a40b2a56caf796f0234a37d3db76d0954c3d2e03ae2fdd13b38198e7ac4"
  end

  depends_on "adwaita-icon-theme"
  depends_on "gtk+3"
  depends_on "py3cairo"
  depends_on "pygobject3"
  depends_on "python@3.9"

  def install
    virtualenv_install_with_resources
  end

  test do
    assert_match version.to_s, shell_output("#{bin}/nicotine -v")
    pid = fork do
      exec bin/"nicotine", "-s"
    end
    sleep 3
    Process.kill("TERM", pid)
  end
end

"""

import "homebrew-install" as hi

import "adwaita-icon-theme" as i0, "atk" as i1, "bison" as i2, "cairo" as i3, "cmake" as i4, "docbook" as i5, "docbook-xsl" as i6, "fontconfig" as i7, "freetype" as i8, "fribidi" as i9, "gdbm" as i10, "gdk-pixbuf" as i11, "gettext" as i12, "glib" as i13, "gobject-introspection" as i14, "graphite2" as i15, "gsettings-desktop-schemas" as i16, "gtk+3" as i17, "harfbuzz" as i18, "hicolor-icon-theme" as i19, "icu4c" as i20, "intltool" as i21, "jpeg" as i22, "libepoxy" as i23, "libffi" as i24, "libpng" as i25, "libpthread-stubs" as i26, "librsvg" as i27, "libssh2" as i28, "libtiff" as i29, "libx11" as i30, "libxau" as i31, "libxcb" as i32, "libxdmcp" as i33, "libxext" as i34, "libxrender" as i35, "lzo" as i36, "meson" as i37, "mpdecimal" as i38, "ninja" as i39, "openssl@1.1" as i40, "pango" as i41, "pcre" as i42, "pixman" as i43, "pkg-config" as i44, "py3cairo" as i45, "pygobject3" as i46, "python@3.9" as i47, "readline" as i48, "rust" as i49, "sphinx-doc" as i50, "sqlite" as i51, "util-macros" as i52, "xcb-proto" as i53, "xorgproto" as i54, "xtrans" as i55, "xz" as i56

pkg(
  name: "nicotine-plus",
  version: "3.0.6",
  dependencies: [hi, i0, i1, i2, i3, i4, i5, i6, i7, i8, i9, i10, i11, i12, i13, i14, i15, i16, i17, i18, i19, i20, i21, i22, i23, i24, i25, i26, i27, i28, i29, i30, i31, i32, i33, i34, i35, i36, i37, i38, i39, i40, i41, i42, i43, i44, i45, i46, i47, i48, i49, i50, i51, i52, i53, i54, i55, i56],

  def install(ctx) {
    ctx.write_file(join(ctx.build, "nicotine-plus.rb"), formula)
    ctx.system("homebrew-install", ctx.prefix, ctx.state_dir, "./nicotine-plus.rb")
  }
)
