formula = r"""
class Afflib < Formula
  desc "Advanced Forensic Format"
  homepage "https://github.com/sshock/AFFLIBv3"
  url "https://github.com/sshock/AFFLIBv3/archive/v3.7.19.tar.gz"
  sha256 "d358b07153dd08df3f35376bab0202c6103808686bab5e8486c78a18b24e2665"
  revision 1

  bottle do
    sha256 cellar: :any, arm64_big_sur: "4ebc86660cab0964031b14ee14a710a8d83222389ba9e263463f7b7610582b3b"
    sha256 cellar: :any, big_sur:       "045dd01683d3c1411e493d72a8b4bcd6e71113386f330254252e5876d702429f"
    sha256 cellar: :any, catalina:      "63075bb1473d3342521c6e29fd1c8c628114cef274ec8b7cc572d46068f19f4a"
    sha256 cellar: :any, mojave:        "9a50d803eedfeb45425b1f7a0452e8f7072d87c2b7b5b488dfca6222a18440c6"
    sha256 cellar: :any, high_sierra:   "9367940cd2b04b6a244b00ba0970ab20b23393604689ee45b5b5b2b5274e752c"
    sha256 cellar: :any, x86_64_linux:  "b3b13766db006faf912918d41cb81809adebe15ae1411a38701c45489e97d842"
  end

  depends_on "autoconf" => :build
  depends_on "automake" => :build
  depends_on "libtool" => :build
  depends_on "pkg-config" => :build
  depends_on "openssl@1.1"
  depends_on "python@3.9"

  uses_from_macos "curl"
  uses_from_macos "expat"

  # Fix for Python 3.9, remove in next version
  patch do
    url "https://github.com/sshock/AFFLIBv3/commit/aeb444da.patch?full_index=1"
    sha256 "90cbb0b55a6e273df986b306d20e0cfb77a263cb85e272e01f1b0d8ee8bd37a0"
  end

  def install
    ENV["PYTHON"] = Formula["python@3.9"].opt_bin/"python3"

    args = %w[
      --enable-s3
      --enable-python
      --disable-fuse
    ]

    system "autoreconf", "-fiv"
    system "./configure", "--disable-dependency-tracking",
                          "--prefix=#{prefix}",
                          *args
    system "make", "install"
  end

  test do
    system "#{bin}/affcat", "-v"
  end
end

"""

import "homebrew-install" as hi

import "autoconf" as i0, "automake" as i1, "berkeley-db" as i2, "bison" as i3, "brotli" as i4, "bzip2" as i5, "c-ares" as i6, "cmake" as i7, "curl" as i8, "expat" as i9, "gdbm" as i10, "gettext" as i11, "jemalloc" as i12, "krb5" as i13, "libev" as i14, "libffi" as i15, "libidn2" as i16, "libssh2" as i17, "libtool" as i18, "libunistring" as i19, "libxml2" as i20, "m4" as i21, "mpdecimal" as i22, "ncurses" as i23, "nghttp2" as i24, "ninja" as i25, "openldap" as i26, "openssl@1.1" as i27, "perl" as i28, "pkg-config" as i29, "python@3.9" as i30, "readline" as i31, "rtmpdump" as i32, "sphinx-doc" as i33, "sqlite" as i34, "unzip" as i35, "xz" as i36, "zip" as i37, "zlib" as i38, "zstd" as i39

pkg(
  name: "afflib",
  version: "3.7.19",
  dependencies: [hi, i0, i1, i2, i3, i4, i5, i6, i7, i8, i9, i10, i11, i12, i13, i14, i15, i16, i17, i18, i19, i20, i21, i22, i23, i24, i25, i26, i27, i28, i29, i30, i31, i32, i33, i34, i35, i36, i37, i38, i39],

  def install(ctx) {
    ctx.write_file(join(ctx.build, "afflib.rb"), formula)
    ctx.system("homebrew-install", ctx.prefix, ctx.state_dir, "./afflib.rb")
  }
)
