formula = r"""
class Mftrace < Formula
  desc "Trace TeX bitmap font to PFA, PFB, or TTF font"
  homepage "https://lilypond.org/mftrace/"
  url "https://lilypond.org/downloads/sources/mftrace/mftrace-1.2.20.tar.gz"
  sha256 "626b7a9945a768c086195ba392632a68d6af5ea24ef525dcd0a4a8b199ea5f6f"
  license "GPL-2.0"
  revision 1

  bottle do
    sha256 cellar: :any_skip_relocation, big_sur:     "09ca3daeb696824e12655d6dbd0c768a0dcc07306c35001bc7b90417fb2b38c6"
    sha256 cellar: :any_skip_relocation, catalina:    "da5fc2002936d2260121ce8134472bce14e8bccfb406fe9e1e56591037aa9751"
    sha256 cellar: :any_skip_relocation, mojave:      "d4b3535bdd69a89c59c4b7d7011ccb06544108c376e6313f62062c32991dece2"
    sha256 cellar: :any_skip_relocation, high_sierra: "e1d8b241eb03982520cf2b4b2f8794fe74afb240247e4ea7c8164b1c9a22e974"
  end

  head do
    url "https://github.com/hanwen/mftrace.git"
    depends_on "autoconf" => :build
  end

  depends_on "fontforge"
  depends_on "potrace"
  depends_on "python@3.9"
  depends_on "t1utils"

  # Fixed in https://github.com/hanwen/mftrace/pull/14
  resource "manpage" do
    url "https://github.com/hanwen/mftrace/raw/release/1.2.20/gf2pbm.1"
    sha256 "f2a7234cba5f59237e3cc1f67e395046b381a012456d4e6e9963673cf35d46fb"
  end

  def install
    buildpath.install resource("manpage") if build.stable?
    system "./autogen.sh" if build.head?
    system "./configure", "--prefix=#{prefix}"
    system "make", "install"
  end

  test do
    system "#{bin}/mftrace", "--version"
  end
end

"""

import "homebrew-install" as hi

import "berkeley-db" as i0, "bison" as i1, "bzip2" as i2, "cairo" as i3, "cmake" as i4, "expat" as i5, "flex" as i6, "fontconfig" as i7, "fontforge" as i8, "freetype" as i9, "fribidi" as i10, "gdbm" as i11, "gettext" as i12, "giflib" as i13, "glib" as i14, "gobject-introspection" as i15, "gperf" as i16, "graphite2" as i17, "harfbuzz" as i18, "help2man" as i19, "icu4c" as i20, "jpeg" as i21, "libffi" as i22, "libpng" as i23, "libpthread-stubs" as i24, "libspiro" as i25, "libtiff" as i26, "libtool" as i27, "libuninameslist" as i28, "libx11" as i29, "libxau" as i30, "libxcb" as i31, "libxdmcp" as i32, "libxext" as i33, "libxml2" as i34, "libxrender" as i35, "lzo" as i36, "m4" as i37, "meson" as i38, "mpdecimal" as i39, "ncurses" as i40, "ninja" as i41, "openssl@1.1" as i42, "pango" as i43, "pcre" as i44, "perl" as i45, "pixman" as i46, "pkg-config" as i47, "potrace" as i48, "python@3.9" as i49, "readline" as i50, "sphinx-doc" as i51, "sqlite" as i52, "t1utils" as i53, "unzip" as i54, "util-macros" as i55, "xcb-proto" as i56, "xorgproto" as i57, "xtrans" as i58, "xz" as i59, "zip" as i60, "zlib" as i61

pkg(
  name: "mftrace",
  version: "1.2.20",
  dependencies: [hi, i0, i1, i2, i3, i4, i5, i6, i7, i8, i9, i10, i11, i12, i13, i14, i15, i16, i17, i18, i19, i20, i21, i22, i23, i24, i25, i26, i27, i28, i29, i30, i31, i32, i33, i34, i35, i36, i37, i38, i39, i40, i41, i42, i43, i44, i45, i46, i47, i48, i49, i50, i51, i52, i53, i54, i55, i56, i57, i58, i59, i60, i61],

  def install(ctx) {
    ctx.write_file(join(ctx.build, "mftrace.rb"), formula)
    ctx.system("homebrew-install", ctx.prefix, ctx.state_dir, "./mftrace.rb")
  }
)
