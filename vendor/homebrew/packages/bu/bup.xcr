formula = r"""
class Bup < Formula
  desc "Backup tool"
  homepage "https://bup.github.io/"
  url "https://github.com/bup/bup/archive/0.32.tar.gz"
  sha256 "a894cfa96c44b9ef48003b2c2104dc5fa6361dd2f4d519261a93178984a51259"
  license all_of: ["BSD-2-Clause", "LGPL-2.0-only"]
  head "https://github.com/bup/bup.git"

  bottle do
    sha256 cellar: :any_skip_relocation, big_sur:  "96005e9af68eb9bc01c01025b693bf25a0fe2aeb2318adaadc643c91f824ea3a"
    sha256 cellar: :any_skip_relocation, catalina: "0509e26be582f806e50a47b36e3656d0031e852dbac6a9a15f500365860111c5"
    sha256 cellar: :any_skip_relocation, mojave:   "d88b558267b83a82fd2dcec7a400558224afaf9a2dd30c910766ee62556e0dbd"
  end

  depends_on "pandoc" => :build
  depends_on "pkg-config" => :build
  depends_on "python@3.9"

  def install
    ENV["PYTHON"] = Formula["python@3.9"].opt_bin/"python3"

    system "make", "PREFIX=#{prefix}", "install"
  end

  test do
    system bin/"bup", "init"
    assert_predicate testpath/".bup", :exist?
  end
end

"""

import "homebrew-install" as hi

import "bzip2" as i0, "cabal-install" as i1, "expat" as i2, "gdbm" as i3, "ghc" as i4, "libffi" as i5, "mpdecimal" as i6, "ncurses" as i7, "openssl@1.1" as i8, "pandoc" as i9, "pkg-config" as i10, "python@3.9" as i11, "readline" as i12, "sphinx-doc" as i13, "sqlite" as i14, "unzip" as i15, "xz" as i16, "zip" as i17, "zlib" as i18

pkg(
  name: "bup",
  version: "0.32",
  dependencies: [hi, i0, i1, i2, i3, i4, i5, i6, i7, i8, i9, i10, i11, i12, i13, i14, i15, i16, i17, i18],

  def install(ctx) {
    ctx.write_file(join(ctx.build, "bup.rb"), formula)
    ctx.system("homebrew-install", ctx.prefix, ctx.state_dir, "./bup.rb")
  }
)
